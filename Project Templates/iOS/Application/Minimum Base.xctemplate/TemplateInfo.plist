<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Kind</key>
        <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
        <key>Identifier</key>
        <string>com.apple.dt.unit.minimumBase</string>
        <key>Ancestors</key>
        <array>
            <string>com.apple.dt.unit.applicationBase</string>
            <string>com.apple.dt.unit.iosBase</string>
            <string>com.apple.dt.unit.languageChoice</string>
        </array>
        <key>Description</key>
        <string>This template provides a starting point for an application that uses a programmatic single view.</string>
        <key>Concrete</key>
        <false/>
        <key>Targets</key>
        <array>
            <dict>
                <key>TargetIdentifier</key>
                <string>com.apple.dt.cocoaTouchApplicationTarget</string>
                <key>SharedSettings</key>
                <dict>
                    <key>ASSETCATALOG_COMPILER_APPICON_NAME</key>
                    <string>AppIcon</string>
                    <key>LD_RUNPATH_SEARCH_PATHS</key>
                    <string>$(inherited) @executable_path/Frameworks</string>
                </dict>
                <key>BuildPhases</key>
                <array>
                    <dict>
                        <key>Class</key>
                        <string>ShellScript</string>
                        <key>ShellPath</key>
                        <string>/bin/sh</string>
                        <key>ShellScript</key>
                        <string>
                            TAGS="TODO:|FIXME:" ERRORTAG="ERROR:" find "${SRCROOT}" -name "*.swift" -print0 | xargs -0 egrep --with-filename --line-number --only-matching "($TAGS).*\$|($ERRORTAG).*\$" | perl -pe "s/($TAGS)/ warning: \$1/" | perl -pe "s/($ERRORTAG)/ error: \$1/"
                        </string>
                    </dict>
                </array>
            </dict>
        </array>
        <key>Options</key>
        <array>
            <!--Device Family-->
            <dict>
                <key>Identifier</key>
                <string>universalDeviceFamily</string>
                <key>Name</key>
                <string>Devices:</string>
                <key>Description</key>
                <string>Which device family to create a project for</string>
                <key>SortOrder</key>
                <integer>1</integer>
                <key>Type</key>
                <string>popup</string>
                <key>Default</key>
                <string>Universal</string>
                <key>Values</key>
                <array>
                    <string>Universal</string>
                    <string>iPhone</string>
                    <string>iPad</string>
                </array>
                <key>Units</key>
                <dict>
                    <key>iPhone</key>
                    <dict>
                        <key>Nodes</key>
                        <array>
                            <string>Info.plist:UISupportedInterfaceOrientations~iPhone</string>
                        </array>
                        <key>Definitions</key>
                        <dict>
                            <key>Assets.xcassets</key>
                            <dict>
                                <key>Path</key>
                                <string>Images-iPhone.xcassets</string>
                                <key>SortOrder</key>
                                <integer>100</integer>
                            </dict>
                        </dict>
                    </dict>
                    <key>iPad</key>
                    <dict>
                        <key>Project</key>
                        <dict>
                            <key>SharedSettings</key>
                            <dict>
                                <key>TARGETED_DEVICE_FAMILY</key>
                                <string>2</string>
                            </dict>
                        </dict>
                        <key>Nodes</key>
                        <array>
                            <string>Info.plist:UISupportedInterfaceOrientations~iPad</string>
                        </array>
                        <key>Definitions</key>
                        <dict>
                            <key>Assets.xcassets</key>
                            <dict>
                                <key>Path</key>
                                <string>Images-iPad.xcassets</string>
                                <key>SortOrder</key>
                                <integer>100</integer>
                            </dict>
                        </dict>
                    </dict>
                    <key>Universal</key>
                    <dict>
                        <key>Project</key>
                        <dict>
                            <key>SharedSettings</key>
                            <dict>
                                <key>TARGETED_DEVICE_FAMILY</key>
                                <string>1,2</string>
                            </dict>
                        </dict>
                        <key>Nodes</key>
                        <array>
                            <string>Info.plist:UISupportedInterfaceOrientations~iPhone</string>
                            <string>Info.plist:UISupportedInterfaceOrientations~iPad</string>
                        </array>
                        <key>Definitions</key>
                        <dict>
                            <key>Assets.xcassets</key>
                            <dict>
                                <key>Path</key>
                                <string>Images-Universal.xcassets</string>
                                <key>SortOrder</key>
                                <integer>100</integer>
                            </dict>
                        </dict>
                    </dict>
                </dict>
            </dict>
            <!--Language Choice-->
            <dict>
                <key>Identifier</key>
                <string>languageChoice</string>
                <key>Units</key>
                <dict>
                    <key>Swift</key>
                    <dict>
                        <key>Project</key>
                        <dict>
                            <key>Configurations</key>
                            <dict>
                                <key>Release</key>
                                <dict>
                                    <key>SWIFT_OBJC_BRIDGING_HEADER</key>
                                    <string>$(PRODUCT_MODULE_NAME)/Bridging.h</string>
                                </dict>
                                <key>Debug</key>
                                <dict>
                                    <key>SWIFT_OBJC_BRIDGING_HEADER</key>
                                    <string>$(PRODUCT_MODULE_NAME)/Bridging.h</string>
                                </dict>
                            </dict>
                        </dict>
                        <key>Nodes</key>
                        <array>
                            <string>Bridging.h</string>
                            <string>Bridging.h:comments</string>
                            <string>Bridging.h:imports:importCocoaTouch</string>
                        </array>
                        <key>Definitions</key>
                        <dict>
                            <key>Bridging.h:imports:importCocoaTouch</key>
                            <string>@import UIKit;</string>
                        </dict>
                    </dict>
                </dict>
            </dict>
        </array>
        <key>Nodes</key>
        <array>
            <string>Info.plist:iPhone</string>
            <string>Info.plist:UIRequiredDeviceCapabilities:base</string>
            <string>Assets.xcassets</string>
        </array>
        <key>Definitions</key>
        <dict>
            <key>Info.plist:iPhone</key>
            <string>
                &lt;key&gt;LSRequiresIPhoneOS&lt;/key&gt; &lt;true/&gt;</string>
            <key>Info.plist:UIRequiredDeviceCapabilities</key>
            <dict>
                <key>Beginning</key>
                <string>
                    &lt;key&gt;UIRequiredDeviceCapabilities&lt;/key&gt; &lt;array&gt;
                </string>
                <key>End</key>
                <string>&lt;/array&gt;</string>
                <key>Indent</key>
                <integer>1</integer>
            </dict>
            <key>Info.plist:UIRequiredDeviceCapabilities:base</key>
            <string>&lt;string&gt;armv7&lt;/string&gt;</string>
            <key>Info.plist:statusBarTintForNavBar</key>
            <string>
                &lt;key&gt;UIStatusBarTintParameters&lt;/key&gt; &lt;dict&gt; &lt;key&gt;UINavigationBar&lt;/key&gt; &lt;dict&gt; &lt;key&gt;Style&lt;/key&gt; &lt;string&gt;UIBarStyleDefault&lt;/string&gt; &lt;key&gt;Translucent&lt;/key&gt; &lt;false/&gt;
                &lt;/dict&gt; &lt;/dict&gt;
            </string>
        </dict>
    </dict>
</plist>
